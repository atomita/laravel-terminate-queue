{
    "name": "atomita/laravel-terminate-queue",
    "description": "",
    "keywords": [
        "atomita",
        "laravel-terminate-queue"
    ],
    "homepage": "https://github.com/atomita/laravel-terminate-queue",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "atomita",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/container": ">=6.0",
        "illuminate/contracts": ">=6.0",
        "illuminate/queue": ">=6.0",
        "illuminate/support": ">=6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "atomita\\LaravelTerminateQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "atomita\\LaravelTerminateQueue\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "atomita\\LaravelTerminateQueue\\ServiceProvider"
            ]
        }
    }
}
